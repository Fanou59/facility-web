generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ajout des mod√®les
model Services{
  id Int @id @default(autoincrement())
  title String
  description String @db.Text
  imageUrl String
  alt String
}

model DetailServices{
  id Int @id @default(autoincrement())
  title String
  description String @db.Text
  imageUrl String
  alt String
  synthese Json
}
